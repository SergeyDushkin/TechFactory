<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  
  <appSettings>
    <add key="host" value="localhost" />
    <add key="port" value="5588" />
  </appSettings>
  
  <connectionStrings>
    <add name="NoodleDb" connectionString="Data Source=partner-db-srv.database.windows.net;Initial Catalog=NoodleDb;User Id=sqladmin@partner-db-srv;Password=!12345Aa;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>

  <nlog>
    <targets>
      <target name="console" type="Console" layout="${longdate} ${message}" />
      <target name="debugger" type="Debugger" layout="${longdate} ${message}" />
    </targets>
    <rules>
      <logger name="*" minlevel="Trace" writeTo="console,debugger" />
    </rules>
  </nlog>
  
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  
